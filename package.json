{
  "name": "gw2-wvw-dashboard",
  "version": "0.0.33",
  "scripts": {
    "prepare": "bun run smui-theme-light && bun run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "pre-run": "bun run prepare",
    "dev": "bun run pre-run && bunx --bun vite",
    "build": "bun run pre-run && vite build",
    "preview": "bun run pre-run && vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig tsconfig.json --watch"
  },
  "devDependencies": {
    "@material/typography": "^14.0.0",
    "@mdi/js": "^7.2.96",
    "@smui/button": "^7.0.0-beta.8",
    "@smui/common": "^7.0.0-beta.8",
    "@smui/drawer": "^7.0.0-beta.8",
    "@smui/icon-button": "^7.0.0-beta.8",
    "@smui/layout-grid": "^7.0.0-beta.8",
    "@smui/linear-progress": "^7.0.0-beta.8",
    "@smui/list": "^7.0.0-beta.8",
    "@smui/paper": "^7.0.0-beta.8",
    "@smui/top-app-bar": "^7.0.0-beta.8",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.12",
    "@types/luxon": "^3.3.0",
    "@types/regression": "^2.0.2",
    "chartjs-adapter-luxon": "^1.3.1",
    "smui-theme": "^7.0.0-beta.8",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.2",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "type": "module",
  "dependencies": {
    "@bun-community/sveltekit-adapter-bun": "^0.0.6",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "chart.js": "^4.3.0",
    "chartjs-plugin-annotation": "^3.0.1",
    "luxon": "^3.3.0",
    "regression": "^2.0.1",
    "rollup": "^3.7.5",
    "svelte-chartjs": "^3.1.2"
  }
}